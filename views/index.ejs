<html>
  <head>
  <link rel="stylesheet" href="style.css">
  </head>

  <body>
    <h1>Hello World</h1>
    <table> 
      <thead>
        <tr>
          <th></th>
          <th>Quantity</th>
          <th>SKU</th>
          <th></th>
          <th>Name</th>
          <th>Price</th>
          <th>Model No.</th>
        </tr>
      </thead>
      <tbody>
        <% for(var key in products) { %>
        <tr id="<%= key %>">
          <td><button onclick="decreaseSKU(<%= key %>)">Decrease by 1.</button>
          <td class="quantity"><%= products[key]["count"] %></td>
          <td><%= products[key]["sku"] %></td>
          <td><img src="<%= products[key]["image"] %>"</td>
          <td><a href=<%= products[key]["url"] %> target="_blank"> <%= products[key]["name"]; %></a></td>
          <td><%= products[key]["price"] %></td>
          <td><%= products[key]["modelnum"] %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
     <script src="client.js"></script> 
  </body>
</html>
